{
  "name": "CookieManager",
  "description": "Cookie管理器",
  "version": "1.0",
  "background": {
    "scripts" : ["background.js"]
  },
  "omnibox": {
    "keyword": "l"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "icons":{
    "16": "icon.png"
  },
  "manifest_version": 2
}